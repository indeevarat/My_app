<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_width="208dp"
            android:layout_height="48dp"
            android:visibility="visible"
            motion:layout_editor_absoluteY="171dp"
            android:id="@+id/etNewName"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="100dp"
            android:layout_marginStart="100dp"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            motion:layout_editor_absoluteY="286dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="98dp"
            android:layout_width="wrap_content"
            android:id="@+id/etNewEmail"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="341dp"
            android:id="@+id/etNewPassword"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="100dp"
            android:layout_marginStart="100dp" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="462dp"
            android:id="@+id/Register"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="144dp"
            android:layout_marginStart="144dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            motion:layout_editor_absoluteY="356dp"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteX="99dp"
            android:layout_width="wrap_content"
            android:id="@+id/etNewPassword" />
    </ConstraintSet>
</MotionScene>